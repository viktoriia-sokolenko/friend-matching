{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "../server/src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "/../server/src/index.js"
      }
    ]
  }